<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="Oxichain" location="startMenu"/>
	<shortcut
		name="Oxichain Server"
		target="$INSTALL_PATH\Oxichain.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start Oxichain server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="Oxichain Wallet"
		target="http://localhost:7376/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch Oxichain wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>